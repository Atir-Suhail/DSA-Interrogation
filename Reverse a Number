#include <bits/stdc++.h>
using namespace std;

int main() {

  int num;
  int new_num = 0;
  int last_digit;

  cout << "Enter an integer: ";
  cin >> num;

  while(num != 0) {
    last_digit = num % 10;
    new_num = new_num * 10 + last_digit;
    num /= 10;
  }

  cout << "new_num  = " << new_num;

  return 0;
}
